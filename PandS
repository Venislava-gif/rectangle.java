import java.util.Scanner;
class RectangleCalc{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
double a,b ,P, S;
System.out.println("Enter a:");
a= sc.nextDouble();
System.out.println("Enter b:");
b =sc.nextDouble();
if(a<=0||b<=0 ){
System.out.println("Invalid rectangle");
return;
}
P=a*2+b*2;
S=a*b;
}
}
